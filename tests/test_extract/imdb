let adFrame = document.querySelector(adElementId);
let ad = {};
if (adFrame) { ad = adFrame.contentDocument.ad; }
const event = {
context: 'ad-feedback',
isProd: true,
placementMetaData: {
pageUrl: pageUrlInBase64,
pageType: 'imdb_title',
slotName: slotName
},
creativeMetaData: {
adElementId: slotName,
adId: ad.aid || '',
adProgramId: '1015',
adNetworkId: 'CS',
creativeId: ad.cid || ''
},
feedbackOriginContext: {
marketplaceId: 'A1EVAM02EL8SFB'
},
};
window.postMessage(event);
});
}
})(window, document);
            </script>
