    BidBarrel.queue.push(function(resolve){
        let __pv = MetaC.handlePageViewCount();

        BidBarrel.initialize({
            dfpPath: "/22309610186/aw-metacritic/tv",
            targeting: {
                auto: true, 
                cookie: {
